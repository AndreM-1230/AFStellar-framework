{
    "name": "afstellar/af-stellar-framework",
    "version": "0.9.0",
    "description": "Lightweight MVC framework with ORM, Migrations and Query Builder",
    "type": "framework",
    "license": "MIT",
    "authors": [
        {
            "name": "Andre Fomichev"
        }
    ],
    "require": {
        "php": "^8.0",
        "afstellar/af-stellar-core": "^0.9.0",
        "vlucas/phpdotenv": "^5.6"
    },
    "autoload": {
      "psr-4": {
        "App\\": "app/",
        "App\\Database\\migrations\\" : "app/Database/migrations/"
      }
    },
    "scripts": {
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    }
}

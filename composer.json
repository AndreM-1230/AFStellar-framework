{
    "name": "afstellar/af-stellar-framework",
    "version": "0.8.4",
    "description": "Lightweight MVC framework with ORM, Migrations and Query Builder",
    "type": "framework",
    "license": "MIT",
    "authors": [
        {
            "name": "Andre Fomichev"
        }
    ],
    "require": {
        "php": "^7.3",
        "afstellar/af-stellar-core": "^0.8.4"
    },
    "autoload": {
      "psr-4": {
        "App\\": "app/",
        "App\\Database\\migrations\\" : "app/Database/migrations/"
      }
    },
    "scripts": {
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    }
}
